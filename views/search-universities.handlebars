<title>Universities Found</title>
<link rel="stylesheet" href="/public/css/site.css" />
<h1>Universities Found</h1>
<h2>Using search term: {{ search }}</h2>
<main>
  <ol>
    {{#each universitySearchResult}}
    <li>
      <a href="search-universities/{{ this._id }}">{{ this.name }}</a>
    </li>
    {{/each}}
  </ol>
</main>
<a href="/">Make Another Search</a>
